<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="SiDePeTra_APPITSG.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:view="clr-namespace:SiDePeTra_APPITSG.views"
    Shell.FlyoutBehavior="Disabled">


    <TabBar>
        <Tab Title="Solicitud" Icon="clipboardfilled.svg">
            <ShellContent ContentTemplate="{DataTemplate view:Solicitud}" />
        </Tab>
        <Tab Title="Configuracion" Icon="settings.svg">
            <ShellContent ContentTemplate="{DataTemplate view:Configuracion}" />
        </Tab>
    </TabBar>
</Shell>
